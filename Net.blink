option ClientOutput = "net/client.luau"
option ServerOutput = "net/server.luau"

struct Transform {
    id: f16,
    dp: vector<f32>,
}
event UpdateTransformDelta {
    From: Server,
    Type: Unreliable,
    Call: SingleSync,
    Poll: false,
    Data: Transform[]
}

event SpawnMob {
    From: Server,
    Type: Reliable,
    Call: SingleSync,
    Poll: true,
    Data: (f16, CFrame, u8)
}